{
  "name": "gemini-project",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "pages:deploy": "wrangler pages deploy out --project-name wordisbond",
    "api:dev": "wrangler dev --config workers/wrangler.toml",
    "api:deploy": "wrangler deploy --config workers/wrangler.toml",
    "api:tail": "wrangler tail wordisbond-api",
    "deploy:all": "npm run env:verify && npm run api:deploy && npm run build && npm run pages:deploy && npm run health-check",
    "cf:tail": "wrangler tail",
    "cf:logs": "wrangler tail --format pretty",
    "cf:secrets": "wrangler secret list",
    "cf:typegen": "wrangler types",
    "db:migrate": "psql $NEON_PG_CONN -f schema.sql",
    "db:reset-test": "psql $NEON_PG_CONN -f migrations/reset_test.sql",
    "db:rls-audit": "psql $NEON_PG_CONN -f scripts/rls-audit.sql",
    "db:rls-enforce": "psql $NEON_PG_CONN -f migrations/2026-02-08-rls-enforcement.sql",
    "db:evidence-views": "psql $NEON_PG_CONN -f migrations/2026-02-09-evidence-immutable-views.sql",
    "db:backup": "bash scripts/neon-backup.sh",
    "test:smoke": "bash scripts/smoke-test.sh",
    "db:schema-check": "bash scripts/schema-drift-check.sh",
    "db:schema-snapshot": "bash scripts/schema-drift-check.sh --snapshot",
    "gen:permissions": "npx tsx tools/generate-permission-matrix.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:live": "vitest --config vitest.production.config.ts --run",
    "test:live:api": "vitest --config vitest.production.config.ts tests/production/api-live.test.ts --run",
    "test:live:db": "vitest --config vitest.production.config.ts tests/production/database-live.test.ts --run",
    "test:live:voice": "vitest --config vitest.production.config.ts tests/production/voice-live.test.ts --run",
    "test:live:all": "vitest --config vitest.production.config.ts --run",
    "test:voice:e2e": "vitest --config vitest.production.config.ts tests/production/voice-e2e.test.ts --run",
    "test:voice:e2e:live": "cross-env ENABLE_LIVE_VOICE_TESTS=true vitest --config vitest.production.config.ts tests/production/voice-e2e.test.ts --run",
    "test:setup:live": "pwsh -File setup-live-tests.ps1",
    "test:setup:live:full": "pwsh -File setup-live-tests.ps1 -EnableLiveCalls",
    "test:translation": "npx tsx scripts/test-live-translation.ts",
    "test:voice-to-voice": "vitest tests/voice-to-voice.test.ts --run",
    "test:validate": "vitest --config vitest.production.config.ts tests/production/feature-validation.test.ts --run",
    "test:validate:functional": "vitest --config vitest.production.config.ts tests/production/functional-validation.test.ts --run",
    "test:validate:all": "vitest --config vitest.production.config.ts tests/production/feature-validation.test.ts tests/production/functional-validation.test.ts --run",
    "test:bridge": "vitest --config vitest.production.config.ts tests/production/bridge-crossing.test.ts --run",
    "test:deep": "vitest --config vitest.production.config.ts tests/production/deep-functional.test.ts --run",
    "test:validate:full": "node tests/production/run-full-suite.js",
    "test:setup:e2e": "node scripts/setup-e2e-tests.js",
    "test:e2e": "npx playwright test",
    "test:e2e:headed": "npx playwright test --headed",
    "test:e2e:ui": "npx playwright test --ui",
    "test:ui-audit": "npx playwright test tests/e2e/ui-endpoint-toggle-audit.spec.ts",
    "test:simulator": "npx playwright test tests/e2e/workplace-simulator.spec.ts --timeout=300000",
    "test:simulator:headed": "npx playwright test tests/e2e/workplace-simulator.spec.ts --headed --timeout=300000",
    "test:simulator:journey": "npx playwright test tests/e2e/workplace-simulator.spec.ts --grep \"Full Journey\" --timeout=300000",
    "test:simulator:workflows": "npx playwright test tests/e2e/workplace-simulator.spec.ts --grep \"Authenticated Workflows\" --timeout=300000",
    "test:simulator:public": "npx playwright test tests/e2e/workplace-simulator.spec.ts --grep \"Public Pages\" --timeout=120000",
    "test:agents:provision": "npx tsx tests/agents/provision-users.ts",
    "test:agents": "npx tsx tests/agents/run.ts",
    "test:agents:headless": "cross-env AGENT_HEADLESS=true npx tsx tests/agents/run.ts",
    "validate": "npx tsx tests/validation/run.ts",
    "validate:security": "npx tsx tests/validation/run.ts --domain security",
    "validate:compliance": "npx tsx tests/validation/run.ts --domain compliance-regf,compliance-tcpa",
    "validate:arch": "npx tsx tests/validation/run.ts --domain arch-alignment,flow-completeness",
    "test:agent:owner": "npx tsx tests/agents/run-single.ts owner",
    "test:agent:admin": "npx tsx tests/agents/run-single.ts admin",
    "test:agent:manager": "npx tsx tests/agents/run-single.ts manager",
    "test:agent:compliance": "npx tsx tests/agents/run-single.ts compliance",
    "test:agent:agent": "npx tsx tests/agents/run-single.ts agent",
    "test:agent:viewer": "npx tsx tests/agents/run-single.ts viewer",
    "test:dialer": "vitest tests/production/dialer-integration.test.ts --run",
    "test:dialer:watch": "vitest tests/production/dialer-integration.test.ts",
    "test:dialer:e2e": "npx playwright test tests/e2e/dialer-workflow.spec.ts",
    "test:dialer:e2e:headed": "npx playwright test tests/e2e/dialer-workflow.spec.ts --headed",
    "test:dialer:all": "npm run test:dialer && npm run test:dialer:e2e",
    "test:coverage": "vitest run --coverage",
    "test:load:smoke": "k6 run tests/load/smoke.k6.js",
    "test:load:baseline": "k6 run tests/load/baseline.k6.js",
    "test:load:spike": "k6 run tests/load/spike.k6.js",
    "test:load:all": "npm run test:load:smoke && npm run test:load:baseline && npm run test:load:spike",
    "arch:validate": "node tools/validate-arch-compliance.js --all",
    "arch:validate:file": "node tools/validate-arch-compliance.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "env:verify": "npx tsx scripts/verify-env.ts",
    "health-check": "curl -f https://wordisbond-api.adrper79.workers.dev/health || exit 1",
    "clean": "rm -rf .next out .wrangler node_modules/.cache",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --no-warn-ignored",
      "prettier --write"
    ],
    "*.{js,jsx,json,md,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.23",
    "@ai-sdk/react": "^3.0.66",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@telnyx/webrtc": "^2.25.17",
    "ai": "^6.0.64",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "lucide-react": "^0.562.0",
    "next": "15.5.7",
    "next-themes": "^0.4.6",
    "papaparse": "^5.5.3",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.4.0",
    "uuid": "^11.1.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@asteasolutions/zod-to-openapi": "^7.3.0",
    "@cloudflare/workers-types": "^4.20260131.0",
    "@eslint/eslintrc": "^3.3.3",
    "@faker-js/faker": "^10.3.0",
    "@neondatabase/serverless": "^1.0.2",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.0.6",
    "@types/papaparse": "^5.5.2",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "15.5.11",
    "eslint-config-prettier": "^10.1.8",
    "glob": "^11.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "pg": "^8.18.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "4.1.18",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "vitest": "^4.0.16",
    "wrangler": "4.61.1"
  },
  "overrides": {
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "next": "15.5.7",
    "typescript": "5.9.3",
    "tailwindcss": "4.1.18"
  }
}
